    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/length": "^7.0.0",
    "@turf/nearest-point-on-line": "^7.0.0",
    "@turf/projection": "^6.5.0",
    "bezier-js": "^6.1.0",
    "colord": "^2.9.3",
    "core-js": "^3.37.0",
